<% provide(:title, 'Log in') %>
<h1>Log in</h1>

<%= form_for(:session, url: login_path) do |f| %>
  <%= f.label :email %>
  <%= f.email_field :email %>
  <br />
  <%= f.label :password %>
  <%= f.password_field :password %>
  <br />
  <%= f.submit 'Log in' %>
<% end %>

<p>
  New user? <%= link_to 'Sign up!', new_user_path %>
</p>
<br />
<%= link_to 'Home', root_path %>
