<h1>Sign in</h1>

<%= form_for :user, url: users_path do |f| %>
  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>
 
  <p>
    <%= f.label :password%>
    <br>
    <%= f.password_field :password %>
  </p>
 
  <p>
    <%= f.submit 'Create user'%>
  </p>
<% end %>

<%= link_to 'Back', login_path %>
